package com.rits;

public class SomeBusinessImpl {

	private DataService dataService;
	
	public int findGreatestFromAllData(){
		int[] data = dataService.retrieveAllData();
		int greatest = Integer.MIN_VALUE;
		for (int value : data) {
			if (value>greatest) {
				greatest = value;
			}
		}
		return greatest;
	}
}
